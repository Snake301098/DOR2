/// @description
function get_ores_name(ores_index){
	switch ores_index
	{
		case 0: return("prometium");
		case 1: return("endurium");
		case 2: return("terbium");
		case 3: return("duranium");
		case 4: return("prometid");
		case 5: return("promerium");
		case 6: return("seprom");
		case 7: return("palladium");
		default: break;
	}
}

